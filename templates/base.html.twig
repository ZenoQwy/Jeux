					{% if is_granted('IS_AUTHENTICATED_FULLY') %}
						<!-- Si user est connecté, et que c'est un admin, le bouton messagerie lui enverra sur la messagerie admin -->
						{% if 'ROLE_ADMIN' in app.user.roles %}
							<a class="me-2 text-decoration-none position-relative" href="{{ path('gestiondesproduits') }}">
								<img class="pt-1" src="{{ asset('images/icones/tools-white.svg') }}">
							</a>
							<a class="me-2 text-decoration-none position-relative px-3" href="{{ path('adminmessagerie') }}">
								<img class="pt-1" src="{{ asset('images/icones/mail-white.svg') }}">
								{% if app.user.receivedSupports is not empty %}
									{% set nbrmessage = app.user.receivedSupports|length %}
									<span class="fs-6 badge bg-primary position-absolute translate-middle p-2" style="border-radius:200px;">{{ nbrmessage }}</span>
								{% endif %}
							</a>
						{% endif %}

						<!-- Si user est connecté, afficher en haut a droite, le canvas menu du compte quand l'icone est cliquée et le logo du panier avec le nombre d'articles mis en panier -->
						<a class="me-2 position-relative text-decoration-none" href="{{path('panier')}}">
							<img class="my-2 my-sm-0 py-1" src="{{asset('images/icones/panier.svg')}}">
							{% if app.user.panier is not null %}
								{% set nbProduitsPanier = 0 %}
								{% for produits in app.user.panier.ajouters %}
									{% set nbProduitsPanier = nbProduitsPanier + produits.getQte() %}
								{% endfor %}
								{% if nbProduitsPanier > 0 %}
									<span class="fs-6 badge bg-primary position-absolute start-100 translate-middle p-2" style="border-radius:200px;">{{nbProduitsPanier}}</span>
								{% endif %}
							{% endif %}
						</a>
						<a class="me-2 text-decoration-none" href="#" data-bs-toggle="offcanvas" data-bs-target="#offcanvasRight" aria-controls="offcanvasRight">
							<img class="px-2 my-2 my-sm-0" src="{{asset('images/icones/logoutilisateur.svg')}}">
						</a>
					{% else %}
						<!--Sinon, l'icone envoie direct sur la page de connexion-->
						<a class="me-2" href="{{path('app_login')}}"><img class="my-2 my-sm-0" src="{{asset('images/icones/logopersonne.svg')}}"></img>
					</a>
